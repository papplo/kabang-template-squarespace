<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes} bg-gray-100">
    <div class="container">
      <header class="header">
        <squarespace:navigation navigationId="mainNav" template="navigation" />
        {.section website}
          <h1 class="site-title-heading" data-content-field="site-title">
            <a href="/" class="site-title-link">{website.siteTitle}</a>
          </h1>
        {.end}

      </header>

      <main class="content" data-content-field="main-content">
        <div>
          {squarespace.main-content}
        </div>
      </main>

      <!--Footer with open block field -->
      <footer class="site-footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

    </div>

    <squarespace:script src="alpine-dist.js" combo="false" />

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}

  </body>
</html>
