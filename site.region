<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    <squarespace:script src="alpine.dist.js" combo="true" />

    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>

  <body class="{squarespace.page-classes}" id="{squarespace.page-id}">
    <div class="container">
      <header class="header">
        <h1 class="site-title">
          <a href="/">
            {website.siteTitle}
          </a>
        </h1>
        <squarespace:navigation navigationId="mainNav" template="navigation" />
      </header>

      <main class="content" data-content-field="main-content">
        <div>
          <p>{website.siteTagLine}</p>
        </div>
      </main>

    </div>

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}

  </body>
</html>
